<% if(month && month.length > 1) { %>

<div class="downloads-chart">
  <div class="graph">
    <div id="download-chart" data-graph-points="<%= JSON.stringify(month) %>"></div>
  </div>
  <div class="day"><%- month[month.length-1].downloads %> download<% if(Number(month[month.length-1].downloads) != 1) { %>s<% } %> yesterday</div>
</div>

<% } %>
